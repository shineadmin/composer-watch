{
	"name": "shineunited/composer-watch",
	"type": "composer-plugin",
	"license": "MIT",
	"require": {
		"php": ">=5.6",
		"composer-plugin-api": "^1.0 || ^2.0",
		"shineunited/composer-build": "^0.1.0",
		"henrikbjorn/lurker": "^1.2"
	},
	"require-dev": {
		"composer/composer": ">=1.0",
		"phpunit/phpunit": ">=5.5.0"
	},
	"autoload": {
		"psr-4": {
			"ShineUnited\\ComposerWatch\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"ShineUnited\\ComposerWatch\\Tests\\": "tests/"
		},
		"extra": {
			"class": "ShineUnited\\ComposerWatch\\Plugin"
		}
	}
}
